public class Login {
	
	WebDriver driver;

	public Login(WebDriver driver) {
		this.driver = driver;
		PageFactory.initElements(driver, this);

	}
	
	@FindBy (name="email")
	WebElement email;
	public WebDriver getDriver() {
		return driver;
	}
